{
  "name": "dochealth",
  "version": "0.1.0",
  "description": "A protocol-driven documentation orchestration tool that uses protocol manifests as the single source of truth to auto-generate documentation, monitor freshness, validate completeness, and route SME reviews intelligently.",
  "main": "bin/dochealth.js",
  "bin": {
    "dochealth": "./bin/dochealth.js"
  },
  "scripts": {
    "test": "node --test tests/**/*.test.js",
    "test:unit": "node --test tests/unit/**/*.test.js",
    "test:integration": "node --test tests/integration/**/*.test.js",
    "test:coverage": "c8 --reporter=text --reporter=html npm test",
    "lint": "eslint lib/ bin/ tests/",
    "format": "prettier --write \"**/*.js\"",
    "type-check": "echo \"No TypeScript configured yet\""
  },
  "keywords": [
    "documentation",
    "cli",
    "protocol",
    "health-check",
    "api-docs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/plugin-retry": "^8.0.3",
    "@octokit/plugin-throttling": "^11.0.3",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0",
    "octokit": "^5.0.5",
    "remark-directive": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "c8": "^8.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
